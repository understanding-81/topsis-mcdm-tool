{
  "builds": [
    {
      "src": "Web/client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "npm run build",
        "distDir": "dist"
      }
    },
    {
      "src": "Web/server/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/Web/server/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/Web/client/dist/$1"
    }
  ]
}
